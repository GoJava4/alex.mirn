<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.go_java4.alex_mirn.model.entity.Quote" table="quote">
        <meta attribute="class-description">
            This class contains the quote detail.
        </meta>
        <id name="id" type="int" column="quote_id">
            <generator class="sequence"/>
        </id>
        <property name="quote" column="quote_name" type="string"/>

    </class>
</hibernate-mapping>